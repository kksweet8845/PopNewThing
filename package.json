{
  "name": "ct_charbot",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "develop": "cross-env NODE_ENV=development npm-run-all --parallel watch nodemon",
    "build:server-js": "cross-env NODE_ENV=development webpack --config webpack.server.config.js",
    "nodemon": "cross-env NODE_ENV=development nodemon --config nodemon.config.json bin/server.bundle.js",
    "watch": "npm-run-all --parallel watch:server-js",
    "watch:server-js": "webpack --watch --config webpack.server.config.js",
    "d": "npm run develop",
    "build:test-js": "cross-env NODE_ENV=development webpack --config webpack.test.config.js",
    "watch:test-js": "webpack --watch --config webpack.test.config.js",
    "nodemon:test-js": "cross-env NODE_ENV=development nodemon bin/test.bundle.js",
    "build:open-test-js": "cross-env NODE_ENV=development webpack --config webpack.open.test.config.js",
    "watch:open-test-js": "webpack --watch --config webpack.open.test.config.js",
    "nodemon:open-test-js": "cross-env NODE_ENV=development nodemon bin/open-test.bundle.js",
    "t": "cross-env NODE_ENV=development npm-run-all --parallel watch:test-js nodemon:test-js",
    "to": "cross-env NODE_ENV=development npm-run-all --parallel build:open-test-js watch:open-test-js nodemon:open-test-js",
    "p": "cp config.js.default config.js",
    "pdb": "cp db/config.js.default db/config.js",
    "watch:fsm-js": "webpack --watch --config webpack.fsm.config.js",
    "nodemon:fsm-js": "cross-env NODE_ENV=development nodemon --config nodemon.config.json bin/fsm.bundle.js",
    "f": "cross-env NODE_ENV=development npm-run-all --parallel watch:fsm-js nodemon:fsm-js",
    "product": "cross-env NODE_ENV=production node bin/server.bundle.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "@google-cloud/storage": "^4.1.3",
    "@line/bot-sdk": "^6.8.3",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-rewire": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "htmlparser2": "^4.0.0",
    "mongoose": "^5.7.14",
    "node-forge": "^0.9.1",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "sleep": "^6.1.0",
    "webpack": "^4.41.2",
    "webpack-node-externals": "^1.7.2",
    "xstate": "^4.7.2"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.12"
  }
}
